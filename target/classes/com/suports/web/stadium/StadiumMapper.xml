<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.suports.web.mapper.StadiumMapper">
<select id="selectStadiumList" resultType="com.suports.web.domain.StadiumDTO">
    SELECT 
   	STADIUM_INDEX,STADIUM_NAME,STADIUM_PHOTO,STADIUM_INFO,AREA_NAME
    FROM STADIUM
    WHERE STADIUM_INDEX LIKE 1
  </select>

</mapper>